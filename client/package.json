{
  "private": true,
  "scripts": {
    "build": "rm -rf dist && mkdir dist && run-p build:* && node scripts/injectCacheLists",
    "build:parcel": "BUILD_TIME=$(node -e 'console.log((new Date).toISOString())') CACHE_FIRST_CACHE=CACHE_FIRST_CACHE NETWORK_FIRST_CACHE=NETWORK_FIRST_CACHE parcel build src/index.html",
    "build:robots": "touch dist/robots.txt",
    "fmt": "npm run test:lint --fix && prettier -u --write '**/*'",
    "icons": "node scripts/icons",
    "start": "BUILD_TIME=$(node -e 'console.log((new Date).toISOString())') parcel src/index.html --open",
    "test": "run-p test:*",
    "test:audit": "npm audit --audit-level=critical",
    "test:e2e": "jest e2e",
    "test:fmt": "prettier --check -u '**/*'",
    "test:lint": "eslint --ext js,ts,tsx .",
    "test:types": "tsc --noEmit",
    "test:unit": "TZ=UTC jest src",
    "test-ci": "run-p test:audit test:fmt test:types test:unit"
  },
  "browserslist": "since 2022 and >1%",
  "dependencies": {
    "@aws-sdk/client-location": "^3.525.0",
    "@aws-sdk/credential-providers": "^3.342.0",
    "@reduxjs/toolkit": "^2.0.1",
    "@sentry/react": "^7.93.0",
    "@tanstack/query-async-storage-persister": "^5.24.1",
    "@tanstack/react-query": "^5.10.0",
    "@tanstack/react-query-persist-client": "^5.4.3",
    "amazon-cognito-identity-js": "^6.1.2",
    "d3-geo": "^3.0.1",
    "d3-interpolate": "^3.0.1",
    "d3-scale": "^4.0.2",
    "date-fns": "^3.0.0",
    "eri": "^45.0.0",
    "file-saver": "^2.0.5",
    "firebase": "^10.11.0",
    "fuse.js": "^7.0.0",
    "idb-keyval": "^6.2.0",
    "marked": "^12.0.0",
    "nosleep.js": "^0.12.0",
    "papaparse": "^5.3.1",
    "react": "^18.3.1",
    "react-dom": "^18.1.0",
    "react-redux": "^9.0.4",
    "react-router-dom": "^6.11.0",
    "topojson-client": "^3.1.0"
  },
  "devDependencies": {
    "@parcel/packager-raw-url": "^2.7.0",
    "@parcel/transformer-webmanifest": "^2.7.0",
    "@types/d3-geo": "^3.0.2",
    "@types/d3-interpolate": "^3.0.1",
    "@types/d3-scale": "^4.0.8",
    "@types/file-saver": "^2.0.1",
    "@types/jest": "^29.5.7",
    "@types/papaparse": "^5.3.8",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.2.8",
    "@types/react-redux": "^7.1.27",
    "@types/topojson-client": "^3.1.1",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "buffer": "^5.7.1",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.5.0",
    "favicons": "^7.1.2",
    "jest": "^29.6.4",
    "npm-run-all": "^4.1.5",
