{
  "private": true,
  "scripts": {
    "build": "rm -rf dist && mkdir dist && run-p build:* && node scripts/injectCacheList",
    "build:parcel": "BUILD_TIME=$(node -e 'console.log((new Date).toISOString())') CACHE_LIST=CACHE_LIST parcel build --no-source-maps src/index.html",
    "build:robots": "touch dist/robots.txt",
    "fmt": "yarn test:lint --fix && prettier --write '**/*'",
    "icons": "node scripts/icons",
    "start": "BUILD_TIME=$(node -e 'console.log((new Date).toISOString())') parcel src/index.html --open",
    "test": "run-p test:*",
    "test:audit": "yarn audit --level critical || [ $? -lt 16 ]",
    "test:e2e": "jest e2e",
    "test:fmt": "prettier --check '**/*'",
    "test:lint": "eslint --ext js,ts,tsx .",
    "test:types": "tsc --noEmit",
    "test:unit": "TZ=UTC jest src",
    "test-ci": "run-p test:audit test:fmt test:types test:unit"
  },
  "browserslist": "cover 85%",
  "dependencies": {
    "@reduxjs/toolkit": "^1.7.2",
    "amazon-cognito-identity-js": "^5.2.6",
    "d3-interpolate": "^3.0.1",
    "date-fns": "^2.28.0",
    "eri": "^38.0.0",
    "file-saver": "^2.0.5",
    "idb-keyval": "^6.0.2",
    "marked": "^4.0.10",
    "nosleep.js": "^0.12.0",
    "papaparse": "^5.3.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.7",
    "react-router-dom": "^6.2.2",
    "react-simple-maps": "^2.3.0"
  },
  "devDependencies": {
    "@parcel/packager-raw-url": "^2.3.1",
    "@parcel/transformer-webmanifest": "^2.3.1",
    "@types/d3-interpolate": "^3.0.1",
    "@types/file-saver": "^2.0.1",
    "@types/jest": "^27.4.1",
    "@types/marked": "^4.0.2",
    "@types/papaparse": "^5.3.2",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.5",
    "@types/react-redux": "^7.1.16",
    "@types/react-simple-maps": "^1.0.7",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "favicons": "^6.2.2",
    "jest": "^27.5.1",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.3.1",
    "path-browserify": "^1.0.1",
    "prettier": "^2.3.0",
    "puppeteer": "^13.3.1",
    "ts-jest": "^27.0.1",
    "typescript": "^4.6.3"
  }
}
