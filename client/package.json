{
  "private": true,
  "scripts": {
    "build": "rm -rf dist && mkdir dist && run-p build:* && node scripts/injectCacheList",
    "build:parcel": "BUILD_TIME=$(node -e 'console.log((new Date).toISOString())') CACHE_LIST=CACHE_LIST parcel build src/index.html",
    "build:robots": "touch dist/robots.txt",
    "fmt": "npm run test:lint --fix && prettier -u --write '**/*'",
    "icons": "node scripts/icons",
    "start": "BUILD_TIME=$(node -e 'console.log((new Date).toISOString())') parcel src/index.html --open",
    "test": "run-p test:*",
    "test:audit": "npm audit --audit-level=low",
    "test:e2e": "jest e2e",
    "test:fmt": "prettier --check -u '**/*'",
    "test:lint": "eslint --ext js,ts,tsx .",
    "test:types": "tsc --noEmit",
    "test:unit": "TZ=UTC jest src",
    "test-ci": "run-p test:audit test:fmt test:types test:unit"
  },
  "browserslist": "cover 80%",
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.5",
    "@sentry/browser": "^7.17.4",
    "amazon-cognito-identity-js": "^5.2.6",
    "d3-geo": "^3.0.1",
    "d3-interpolate": "^3.0.1",
    "date-fns": "^2.29.2",
    "eri": "^41.0.0",
    "file-saver": "^2.0.5",
    "fuse.js": "^6.6.2",
    "idb-keyval": "^6.2.0",
    "marked": "^4.2.0",
    "nosleep.js": "^0.12.0",
    "papaparse": "^5.3.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-query": "^3.39.2",
    "react-redux": "^8.0.1",
    "react-router-dom": "^6.2.2",
    "topojson-client": "^3.1.0"
  },
  "devDependencies": {
    "@parcel/packager-raw-url": "^2.7.0",
    "@parcel/transformer-webmanifest": "^2.7.0",
    "@types/d3-geo": "^3.0.2",
    "@types/d3-interpolate": "^3.0.1",
    "@types/file-saver": "^2.0.1",
    "@types/jest": "^29.0.0",
    "@types/marked": "^4.0.6",
    "@types/papaparse": "^5.3.5",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.3",
    "@types/react-redux": "^7.1.16",
    "@types/topojson-client": "^3.1.1",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "buffer": "^5.7.1",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-react": "^7.31.1",
    "eslint-plugin-react-hooks": "^4.5.0",
    "favicons": "^7.0.0-beta.5",
    "jest": "^29.1.2",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.7.0",
    "path-browserify": "^1.0.1",
    "prettier": "^2.3.0",
    "process": "^0.11.10",
    "puppeteer": "^19.2.0",
    "sharp": "^0.31.2",
    "ts-jest": "^29.0.3",
    "typescript": "^4.6.4"
  }
}
